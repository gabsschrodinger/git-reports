{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist",
    "target": "ES6",
    "moduleResolution": "node",
    "preserveConstEnums": true,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "strict": true
  },
  "exclude": ["node_modules"]
}
